<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="OcampoElective2Project.Views.RegistrationPage">
    <ContentPage.Content>
        <StackLayout>
            <StackLayout Orientation="Horizontal">
                <Label Text="First Name" Margin="2,0"></Label>
                <Entry WidthRequest="150"></Entry>
            </StackLayout>
            <StackLayout Orientation="Horizontal">
                <Label Text="Last Name" Margin="2,0"></Label>
                <Entry WidthRequest="150"></Entry>
            </StackLayout>
            <StackLayout Orientation="Horizontal">
                <Label Text="Birthday" Margin="2,0"></Label>
                <DatePicker></DatePicker>
            </StackLayout>
            <StackLayout Orientation="Horizontal">
                <Label Text="Email AdDRESS"></Label>
                <Entry WidthRequest="150"></Entry>
            </StackLayout>
            <StackLayout Orientation="Horizontal">
                <Label Text="Password"></Label>
                <Entry WidthRequest="150"></Entry>
            </StackLayout>
            <StackLayout Orientation="Horizontal">
                <Label Text="Retype Password"></Label>
                <Entry WidthRequest="150"></Entry>
            </StackLayout>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>